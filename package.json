{
  "name": "admin-pro",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:report": "vue-cli-service build --report",
    "stage": "vue-cli-service build --mode staging",
    "serve:global": "npm install -g serve",
    "preview": "yarn build && serve -s dist",
    "test": "vue-cli-service build --mode test",
    "dll": "webpack --config ./webpack.dll.config.js",
    "dll:dev": "yarn dll && yarn serve",
    "dll:build": "yarn dll && yarn build",
    "generateAnalyzFile": "vue-cli-service build --mode production --report-json",
    "analyz": "webpack-bundle-analyzer --port 8888 ./dist/report.json",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lint:js": "eslint --fix src/**/*.{js,jsx,ts,tsx,vue}",
    "lint:prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css,less,scss,vue,html,md,json}",
    "lint:style": "stylelint --fix src/**/*.{html,vue,css,sass,scss,less}",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{html,vue,css,sass,scss,less}": [
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,css,less,scss,vue,html,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ztree/ztree_v3": "^3.5.46",
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "clipboard": "^2.0.8",
    "core-js": "^3.10.1",
    "dayjs": "^1.10.4",
    "echarts": "^5.1.0",
    "element-ui": "^2.15.1",
    "enquire.js": "^2.1.6",
    "jquery": "^3.6.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "qs": "^6.10.1",
    "regenerator-runtime": "^0.13.7",
    "screenfull": "^5.1.0",
    "spark-md5": "^3.0.1",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-plugin-router": "~4.5.12",
    "@vue/cli-plugin-vuex": "~4.5.12",
    "@vue/cli-service": "~4.5.12",
    "add-asset-html-webpack-plugin": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.3",
    "compression-webpack-plugin": "6.1.1",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "4.3.8",
    "lint-staged": "^10.5.4",
    "lodash-webpack-plugin": "^0.11.6",
    "mockjs": "^1.1.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.10",
    "sass-loader": "10.1.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "standard-version": "^9.2.0",
    "stylelint": "^13.12.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "svg-sprite-loader": "^6.0.5",
    "svgo-loader": "^2.2.2",
    "vue-template-compiler": "^2.6.12",
    "vuex-persist": "^3.1.3",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.6.0",
    "webpack-theme-color-replacer": "1.3.18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
